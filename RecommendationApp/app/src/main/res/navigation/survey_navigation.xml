<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/survey_navigation"
    app:startDestination="@id/multiselectQuestionFragment">

    <fragment
        android:id="@+id/multiselectQuestionFragment"
        android:name="com.thesis.recommendationapp.survey.MultiselectQuestionFragment"
        android:label="Survey"
        tools:layout="@layout/fragment_multiselect_question" >
        <action
            android:id="@+id/action_multiselectQuestionFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultFragment"
        android:name="com.thesis.recommendationapp.resorts.ResultFragment"
        android:label="Recommendation"
        tools:layout="@layout/fragment_result" />
</navigation>